#include <iostream> 
#include <iomanip>
#include <cmath>
using namespace std;

int main ()
{
int A, B, Y, D, E;
cin >> A >> B;
if (A <1 || A >1000 || B <1 || B >1000) 
return 0;
float C= A/2.0;
int X = A/2.0;
if (C == X) 
Y= 1;
else 
Y= 0;
float R= B/2.0;
int W = B/2.0;
if (R == W) 
E= 1;
else 
E= 0;
if (E==Y)
D= 1;
else 
D=0;
cout << D << endl;
}
